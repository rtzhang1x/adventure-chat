<html lang="en">
<%- include('./header.ejs'); %>
    <body>
        <h1>Login Page</h1>
        <form>
            <div class="error" style="background-color: red;"></div><br />
            <label for="username">Username</label><br />
            <input type="text" id="user" required /><br />
